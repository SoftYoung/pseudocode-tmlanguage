{
  "name": "syntaxes-pseudocode-tmlanguage-json",
  "publisher": "TFerreira25",
  "displayName": "PseudoRunner - Pseudocode Grammar",
  "description": "TextMate grammar for .pseudo files used by the PseudoRunner interpreter. Adds syntax highlighting for pseudocode (keywords, comments, strings, numbers, operators, arrays and functions).",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/TFerreira25/pseudocode-tmlanguage.git"
  },
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pseudocode",
        "aliases": [
          "Pseudocode"
        ],
        "extensions": [
          ".pseudo"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pseudocode",
        "scopeName": "source.pseudocode",
        "path": "./syntaxes/pseudocode.tmLanguage.json"
      }
    ]
  }
}
